<% provide(:page_title, 'My Posts') %>
<% provide(:user_active, 'active') %>

<div class="row">
    <%= link_to 'New Post', new_users_post_path, class: 'btn btn-primary btn-lg' %>
</div>

<br>
<div class="row">
  <table class="table table-striped">
    <thead>
      <tr>
        <th colspan="3">Title</th>
      </tr>
    </thead>
    <tbody>
      <%= render @posts %>
    </tbody>
  </table>
</div>